.overlay 
    .modal.modal__subscribe#subscribe-modal 
        svg.modal__close(width="22" height="22")
            use(xlink:href="#close-modal-icn")
        .modal__head
            h4.modal__title Подписаться на рассылку
            p.modal__subtitle Введите вашу почту
        form.modal__form 
            .modal__form-content
                .modal__row 
                    label.modal__label Почта
                    .modal__input 
                        input.input(type="text" placeholder="@")
            button(type='submit').btn.btn--gradient Подписаться

    .modal.modal__subscribe#subscribe-modal__status-active
        svg.modal__close(width="22" height="22")
            use(xlink:href="#close-modal-icn")
        .modal__status
            svg(width="31" height="31")
                use(xlink:href="#check-green-icn")
            span Удалось оформить подписку

        button(type='submit').btn.btn--gradient.btn-modal-confirm Подтвердить

    .modal.modal__subscribe#subscribe-modal__status-error
        svg.modal__close(width="22" height="22")
            use(xlink:href="#close-modal-icn")
        .modal__status
            svg(width="31" height="31")
                use(xlink:href="#check-error-icn")
            span Не удалось оформить подписку

        button(type='submit').btn.btn--gradient.btn-modal-confirm Подтвердить

